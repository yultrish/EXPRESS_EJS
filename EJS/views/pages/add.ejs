        <main>
            <h1>Add new product</h1>

            <div class="date">
                <label for="date" class=" hidden">date</label>
                <input type="date" id="date">
            </div>

            <div class="container mt-4">
                <form class="shadow-2xl rounded-2xl p-4" enctype="multipart/form-data">

                    <div class="mb-3">
                        <div class="flex justify-between items-center mx-auto w-full p-2">
                            <label for="mealName" class="mr-auto">Meal name</label>
                            <div></div>
                        </div>
                        <input type="text" class="form-control" id="mealtName">
                    </div>
                    <div class="mb-3">
                        <div class="flex justify-between items-center mx-auto w-full p-2">
                            <label for="description" class="mr-auto">Description</label>
                            <div></div>
                        </div>
                        <input type="text" class="form-control" id="description">
                    </div>
                    <div class="mb-3">
                        <div class="flex justify-between items-center mx-auto w-full p-2">
                            <label for="price" class="mr-auto">Price</label>
                            <div></div>
                        </div>
                        <input type="number" class="form-control" id="price">
                    </div>
                    <div class="mb-3">
                        <div class="flex justify-between items-center mx-auto w-full p-2">
                            <label for="stock" class="mr-auto">Stock</label>
                            <div></div>

                        </div>
                        <input type="number" class="form-control" id="vegetarian">
                    </div>
                    <div class="input-group mb-3">
